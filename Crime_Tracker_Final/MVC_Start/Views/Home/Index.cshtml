                    <div id="rev_slider_202_1" class="rev_slider" style="display:none;" data-version="5.1.1RC">
                        <ul>
                            <!-- SLIDE  -->
                            <li data-index="rs-672">
                                <!-- MAIN IMAGE -->
                                <img src="images/BannerA.jpg" alt="" class="rev-slidebg" data-no-retina>
                            </li>
                            <li data-index="rs-672">
